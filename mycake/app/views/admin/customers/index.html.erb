<div class="container">
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-7">
      <br><h5>会員一覧</h5><br><br>
      <table class="table">
        <thead>
          <tr><th>会員ID</th><th>氏名</th><th>メールアドレス</th><th>ステータス</th></tr>
        </thead>
        <tbody>
          <% @customers.each do |xx|%>
          <tr>
            <td><%= xx.id%></td>
            <td><%= xx.last_name%><%= xx.first_name%></td>
            <td><%= xx.email%></td>
            <td>
              <% if xx.is_active%>
              有効
              <% else %>
              退会
              <%end%>
            </td>
          </tr>
          <%end%>
        </tbody>
      </table>
    </div>