<div class="container">
  <div class="row"><div class="col"><h5>管理者ログイン</h5></div></div>

<div class="row">
<div class="col-md-4"></div>
<div class="col-md-7"><br>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div>メールアドレス<%= f.email_field :email, autofocus: true, autocomplete: "email" %></div>

  <div>パスワード　　<%= f.password_field :password, autocomplete: "current-password" %></div><br>

  <div class="actions">
    <%= f.submit "ログイン", class: "btn btn-sm btn-primary" %>
  </div>
<% end %>

<%= render "admin/shared/links" %>
</div>
</div>
</div>
